{
    "name": "resend/laravel",
    "description": "",
    "keywords": ["php", "resend", "laravel", "sdk", "api", "client"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jayan Ratna",
            "email": "jayan@pixelet.co"
        },
        {
            "name": "Resend and contributors",
            "homepage": "https://github.com/jayanratna/resend-php/contributors"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "illuminate/support": "^9.21|^10.0",
        "resend/client": "^0.3.0",
        "symfony/mailer": "^6.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.14",
        "orchestra/testbench": "^7.22|^8.0",
        "pestphp/pest": "^1.22",
        "pestphp/pest-plugin-mock": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Resend\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Resend\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Resend\\Laravel\\ResendServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
